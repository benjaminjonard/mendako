{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",  
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "2.7.0",
        "doctrine/doctrine-migrations-bundle": "3.2.2",
        "doctrine/orm": "2.13.3",
        "gedmo/doctrine-extensions": "3.9.0",
        "php-ffmpeg/php-ffmpeg": "1.0.1",
        "sensio/framework-extra-bundle": "6.2.8",
        "stof/doctrine-extensions-bundle": "1.7.0",
        "symfony/asset": "6.1.5",
        "symfony/browser-kit": "6.1.3",
        "symfony/console": "6.1.6",
        "symfony/css-selector": "6.1.3",
        "symfony/dependency-injection": "6.1.5",
        "symfony/dotenv": "6.1.0",
        "symfony/event-dispatcher": "6.1.0",
        "symfony/expression-language": "6.1.6",
        "symfony/flex": "2.2.3",
        "symfony/form": "6.1.5",
        "symfony/framework-bundle": "6.1.6",
        "symfony/mime": "6.1.6",
        "symfony/monolog-bundle": "3.8.0",
        "symfony/phpunit-bridge": "6.1.6",
        "symfony/proxy-manager-bridge": "6.1.0",
        "symfony/routing": "6.1.5",
        "symfony/runtime": "6.1.3",
        "symfony/security-bundle": "6.1.3",
        "symfony/string": "6.1.6",
        "symfony/translation": "6.1.6",
        "symfony/twig-bundle": "6.1.1",
        "symfony/uid": "6.1.5",
        "symfony/validator": "6.1.6",
        "symfony/webpack-encore-bundle": "1.16.0",
        "symfony/yaml": "6.1.6",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "3.4.2",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "brianium/paratest": "6.6.5",
        "dama/doctrine-test-bundle": "7.1.1",
        "phpunit/php-code-coverage": "9.2.18",
        "phpunit/phpunit": "9.5.26",
        "symfony/debug-bundle": "6.1.3",
        "symfony/maker-bundle": "1.47.0",
        "symfony/stopwatch": "6.1.5",
        "symfony/web-profiler-bundle": "6.1.6",
        "zenstruck/foundry": "1.22.1"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test:coverage": [
            "@putenv XDEBUG_MODE=coverage",
            "./vendor/phpunit/phpunit/phpunit --color=always --coverage-html=\"output/code-coverage\""
        ],
        "test:phpunit": [
            "./vendor/phpunit/phpunit/phpunit"
        ],
        "test:paratest": [
            "./vendor/bin/paratest --runner=WrapperRunner"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    }
}
