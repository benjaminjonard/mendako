{
    "name": "benjaminjonard/mendako",
    "description": "Koillection is a self-hosted service allowing users to manage any kind of collection.",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",  
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "contao/imagine-svg": "^1.0",
        "doctrine/doctrine-bundle": "2.11.1",
        "doctrine/doctrine-migrations-bundle": "3.3.0",
        "doctrine/orm": "2.17.1",
        "gedmo/doctrine-extensions": "3.14.0",
        "php-ffmpeg/php-ffmpeg": "1.2.0",
        "stof/doctrine-extensions-bundle": "1.10.1",
        "symfony/asset": "7.0.0",
        "symfony/browser-kit": "7.0.0",
        "symfony/cache": "7.0.0",
        "symfony/console": "7.0.0",
        "symfony/css-selector": "7.0.0",
        "symfony/dependency-injection": "7.0.0",
        "symfony/dotenv": "7.0.0",
        "symfony/event-dispatcher": "7.0.0",
        "symfony/expression-language": "7.0.0",
        "symfony/flex": "2.4.1",
        "symfony/form": "7.0.0",
        "symfony/framework-bundle": "7.0.0",
        "symfony/mime": "7.0.0",
        "symfony/monolog-bundle": "3.10.0",
        "symfony/phpunit-bridge": "7.0.0",
        "symfony/proxy-manager-bridge": "6.4.0",
        "symfony/routing": "7.0.0",
        "symfony/runtime": "7.0.0",
        "symfony/security-bundle": "7.0.0",
        "symfony/string": "7.0.0",
        "symfony/translation": "7.0.0",
        "symfony/twig-bundle": "7.0.0",
        "symfony/uid": "7.0.0",
        "symfony/validator": "7.0.0",
        "symfony/webpack-encore-bundle": "2.1.1",
        "symfony/yaml": "7.0.0",
        "twig/extra-bundle": "3.8.0",
        "twig/intl-extra": "3.8.0",
        "twig/twig": "3.8.0"
    },
    "require-dev": {
        "brianium/paratest": "7.3.1",
        "dama/doctrine-test-bundle": "8.0.0",
        "phpunit/php-code-coverage": "10.1.9",
        "phpunit/phpunit": "10.4.2",
        "rector/rector": "0.18.12",
        "symfony/debug-bundle": "7.0.0",
        "symfony/maker-bundle": "1.51.1",
        "symfony/stopwatch": "7.0.0",
        "symfony/web-profiler-bundle": "7.0.0",
        "zenstruck/foundry": "1.36.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test:coverage": [
            "@putenv XDEBUG_MODE=coverage",
            "Composer\\Config::disableProcessTimeout",
            "./vendor/phpunit/phpunit/phpunit --color=always --coverage-html=\"output/code-coverage\""
        ],
        "test:phpunit": [
            "Composer\\Config::disableProcessTimeout",
            "./vendor/phpunit/phpunit/phpunit"
        ],
        "test:paratest": [
            "Composer\\Config::disableProcessTimeout",
            "./vendor/bin/paratest --runner=WrapperRunner"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "docker": false,
            "require": "7.0.*"
        }
    }
}
